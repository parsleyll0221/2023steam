<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.publicvoid.mapper.PublisherMapper">
	<insert id="insert">
		insert into publisher(publisher_name) values(#{publisherName})
	</insert>
	
	<select id="selectByPublisherName" resultType="PublisherVO">
		select * from publisher where publisher_name = #{publisher_name}
	</select>
</mapper>